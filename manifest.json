{

    "name": "Stormy- Send message without saving contact",
    "version": "1.1",
    "description": "Sends Whatsapp Message to a phone Number without adding it.",
    "permissions": ["contextMenus"],
    "manifest_version": 2,
    "icons": {
        "16": "./assets/extension-icons/logo-16.png",
        "128": "./assets/extension-icons/logo-128.png",
        "48": "./assets/extension-icons/logo-48.png"
    },
    "page_action": {
        "default_icon": "./assets/extension-icons/logo-16.png",
        "default_title": "Stormy - send Whatsapp Message without saving number"
    },
    "background": {
        "scripts": ["libPhoneNumber.js","eventPage.js"],
        "persistent": false
    }
}
